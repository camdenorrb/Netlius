{
  "packages": {
    ".": {
      "release-type": "simple",
      "package-name": "netlius",
      "extra-files": [
        {
          "type": "regex",
          "path": "build.gradle.kts",
          "search": "version = \"([0-9]+\\.[0-9]+\\.[0-9]+)\"",
          "replace": "version = \"${version}\""
        }
      ]
    }
  }
}
